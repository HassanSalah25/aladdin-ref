{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "artesaos/seotools": "^1.2",
        "austintoddj/canvas": "^6.0",
        "biscolab/laravel-recaptcha": "^6.0",
        "cmgmyr/messenger": "^2.29",
        "codebyray/laravel-review-rateable": "^1.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/database": "^8.83",
        "instamojo/instamojo-php": "^1.0",
        "intervention/image": "^2.7",
        "joedixon/laravel-translation": "^2.2",
        "laravel/framework": "^8.75",
        "laravel/helpers": "^1.6",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "laravelista/comments": "^4.5",
        "laravelium/sitemap": "^8.0",
        "mews/purifier": "^3.4",
        "nicolaslopezj/searchable": "^1.13",
        "razorpay/razorpay": "^2.8",
        "spatie/opening-hours": "^2.13",
        "srmklive/paypal": "^3.0",
        "stripe/stripe-php": "^10.13",
        "tymon/jwt-auth": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10"
    },
    "autoload": {
	"files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
